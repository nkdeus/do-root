{{> head}}
{{> nav}}

<script type="text/javascript" src="{{{rootPath}}}js/do/modules/do-module-tags.js"></script>

<section class="por h-auto do-px-smart-xxxl do-pt-xxl do-pb-md por do-z-1 do-h-10-vh">
    <div class="do-flex do-x do-start-center por">
        <a class="do-p-sm poa left-n-xs-rem do-br-xl do-bg-extra-color-main-grad-linear-3 trans-y-middle-x-before " href="../do.html">
            <div class="do-wh-xs poa top-middle left trans-y-middle-x-before do-z-2">
                {{> assets/icones/prev}}
            </div>

            <span class="do-z-2 do-fs-sm por do-txt-main-contraste-auto do-fw-light">do</span>
            <div class="do-z-1 do-full do-br-xl do-bg-second-color poa top-n-xl left-n-xl"></div>  
        </a>
        <h1>1001 Class</h1>
    </div>
    <p class="do-max-w-md">Lorem ipsum dolor sit amet consectetur adipisicing elit. Similique saepe ducimus ex cupiditate suscipit esse nostrum mollitia repellendus minima. Quia nemo ipsum fugiat est? Sunt quas repudiandae pariatur quos tenetur.</p>

</section>

<div class="do-flex do-x do-gap-xs do-px-smart-xxxl do-w-9 pos top do-z-10 do-start-center">
    <input id="rechercheArea" class="do-p-xs reset no-focus do-txt-main-color-auto do-bg-main-color-darken do-fs-xs" type="text" name="class" placeholder="Recherche">
    <nav id="inside-page-nav" class="do-flex do-x do-gap-xs">
        {{#classDo}}
        <a class="cursor-pointer do-bg-second-color do-txt-contraste-color active:do-txt-second-color do-p-xs active:do-bg-contraste-color do-bg-second-color-lighten:hover" data-do-id-section-page="{{name}}">{{name}}</a>
        {{/classDo}}
    </nav>
</div>

<div
  data-module="dofilters"
  data-do-stage="#class"
  data-do-target=".page-section"
  data-do-nav="#inside-page-nav"
  data-do-bt-attr="data-do-id-section-page"
  data-do-custom-data="data-do-id-section-page"
  data-do-attr="id"
  data-do-sep=""
  data-do-type="solo"
  data-do-tag-index="0"
  data-do-class-toggle="all-active:do-hide"
>
</div>


<div 
  data-module="doscrolldatas"
  data-do-target="#class"
  data-do-trigger-calcul="body"
  data-do-calcul="full"
  data-do-update="true"
  data-do-start="top-=0% center"
  data-do-end="bottom-=0% bottom"
  data-do-force="1"
  >
</div>

<section id="class" class="por do-px-smart-xxxl h-auto do-py-xxl por do-z-1 do-flex do-y do-gap-xl">

    {{#classDo}}

        <div id="{{name}}" class="page-section">
            <h2 class="do-mb-md do-fs-fluid-lg">{{name}}</h2>
  
            <div class="do-grid-3 do-gap-md">
                {{#group}}
                    <div class="do-bg-second-color-lighten">
                        <div class="do-p-xs  por">
                            <h3 class="do-block do-fw-light"><span class="do-o-5">.do</span>-<span class="do-bg-main-color do-txt-main-color-auto do-fw-regular">{{name}}</span>-<span class="do-bg-extra-color-darken">size</span></h3> 
                            {{#props}}
                                <span class="do-inline-block do-fw-bold  do-fs-xxs uppercase">{{.}}</span>  
                            {{/props}}
                            
                            <span class="do-inline-block do-fw-bold uppercase do-fs-xxs do-bg-extra-color-darken">{{unit}}</span>  
                            
                        </div>
                        <div class="do-gap-xxs do-grid-3 do-fs-xs do-p-xs">
                            {{#pageDo.sizes}}
                            <span class="do-block do-fw-light"><span class="do-o-5">.do</span>-<span class="do-bg-main-color do-txt-main-color-auto do-fw-regular">{{name}}</span>-<span class="do-bg-extra-color-darken">{{size}}</span></span>   
                            {{/pageDo.sizes}}
                        </div>
                    </div>
                {{/group}}
            </div>

   
        </div>

    {{/classDo}}

</section>




{{> footer}}
{{> foot}}
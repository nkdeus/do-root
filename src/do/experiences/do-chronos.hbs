{{> head pageName='do-chronos'}}
{{> nav}}

<script type="text/javascript" src="{{asset 'js/do/modules/do-module-chronos.js'}}"></script>

<section class="por h-auto do-px-smart-xxxl do-pt-xxl do-pb-md por do-z-1 do-h-10-vh">
    <div class="do-flex do-x do-start-center por">
        <a class="do-p-sm poa left-n-xs-rem do-br-xl do-bg-extra-color-main-grad-linear-3 trans-y-middle-x-before " href="../experiences.html">
            <div class="do-wh-xs poa top-middle left trans-y-middle-x-before do-z-2">
                {{> assets/icones/prev}}
            </div>

            <span class="do-z-2 do-fs-sm por do-txt-main-contraste-auto do-fw-light">do</span>
            <div class="do-z-1 do-full do-br-xl do-bg-second-color poa top-n-xl left-n-xl"></div>  
        </a>
        <h1>do-chronos</h1>
    </div>
    <p class="do-w-max-lg">
        <a target="_blank" href="{{asset 'js/do/modules/do-module-chronos.js'}}">{{asset 'js/do/modules/do-module-chronos.js'}}</a> 
    </p>

</section>


<div 
  data-module="doscrolldatas"
  data-do-target="#class"
  data-do-trigger-calcul="body"
  data-do-calcul="full"
  data-do-update="true"
  data-do-start="top-=0% center"
  data-do-end="bottom-=0% bottom"
  data-do-force="1"
  >
</div>

<section id="class" class="por do-w-10 do-px-smart-xxxl h-auto do-py-xxl por do-z-1 do-flex do-y do-start-start do-gap-xl">


    <div class="page-section do-pr-xl xl:do-pr-xxxs do-w-10">

        <h3 class="do-mb-md do-fs-fluid-lg">annees</h3>
        <span data-module="dochronos" data-do-chronos="annees">designer since <span class="do-date">0</span> years</span>
            
    </div>

    <div class="page-section do-pr-xl xl:do-pr-xxxs do-w-10">

        <h3 class="do-mb-md do-fs-fluid-lg">epoque ( no trigger )</h3>
        <span data-module="dochronos" data-do-chronos="epoque" data-do-notrigger="true">in <span class="do-date">2004</span></span>
            
    </div>

        <div class="page-section do-pr-xl xl:do-pr-xxxs do-w-10">

        <h3 class="do-mb-md do-fs-fluid-lg">epoque</h3>
        <span data-module="dochronos" data-do-chronos="epoque">in <span class="do-date">0</span></span>

            
    </div>

    
        <div class="page-section do-pr-xl xl:do-pr-xxxs do-w-10">

        <h3 class="do-mb-md do-fs-fluid-lg">epoque, custom data-do-date="2000-01-01T00:00"</h3>
        <span data-module="dochronos" data-do-chronos="epoque" data-do-date="2000-01-01T00:00">in <span class="do-date">0</span></span>

            
    </div>

    <div class="page-section do-pr-xl xl:do-pr-xxxs do-w-10">

        <h3 class="do-mb-md do-fs-fluid-lg">number, data-do-number="42"</h3>
        <span data-module="dochronos" data-do-start="bottom bottom+=10%" data-do-end="bottom bottom" data-do-chronos="number" data-do-number="42">dans <span class="do-date">0</span> minutes</span>

            
    </div>






    

</section>




{{> footer}}
{{> foot}}
{{> head}}
{{> nav}}

<section class="por do-px-smart-xxxl do-py-xxl por do-z-1 do-h-10-vh">
    <div class="get-progress:trans-y-xxxl">
        <h1 class="get-progress:o-reverse">{{pageHome.title}}</h1>
        <p class="get-progress:o-reverse">{{pageHome.text}}</p>
    </div>
</section>

<div class="do-hide"
    data-module="_domousedatas"
    data-do-target-id="cercle01"
    data-do-calcul="go-in"
    data-do-rayon="2" 
    data-do-offset="0" 
    data-do-force="1"
    >
</div>


{
  "devDependencies": {
    "gulp": "^4.0.2",
    "gulp-clean-css": "^4.3.0",
    "gulp-concat": "^2.6.1",
    "gulp-mustache": "^5.0.0",
    "gulp-purgecss": "^4.1.3",
    "gulp-rename": "^2.0.0",
    "gulp-uglify": "^3.0.2",
    "mustache": "^4.2.0",
    "sass": "^1.49.9"
  },
  "scripts": {
    "css:watch": "sass --watch scss:css",
    "css:compil": "sass scss/style.scss css/style.css",
    "css:purge": "purgecss --config purgecss.config.js",
    "css:refresh": "npm run css:compil && npm run css:purge",
    "build": "gulp build",
    "git": "git add . && git commit -m",
    "postgit": "git push --all",
    "render": "npm run css:compil && npm run build && npm run css:purge",
    "lunch": "npm run git -- lunch"
  },
  "dependencies": {
    "gulp-merge-json": "^2.1.1"
  }
}

{
  "devDependencies": {
    "gulp": "^4.0.2",
    "gulp-clean-css": "^4.3.0",
    "gulp-concat": "^2.6.1",
    "gulp-merge-json": "^2.1.1",
    "gulp-mustache": "^5.0.0",
    "gulp-rename": "^2.0.0",
    "gulp-uglify": "^3.0.2",
    "gulp-watch": "^5.0.1",
    "mustache": "^4.2.0",
    "purgecss": "^4.1.3",
    "sass": "^1.49.9"
  },
  "scripts": {
    "css:watch": "sass --watch sources/scss:sources/css",
    "css:compil": "sass sources/scss/style.scss sources/css/do.css",
    "cssMerge": "gulp cssMerge",
    "css:purge": "purgecss --config purgecss.config.js",
    "css:refresh": "npm run cssMerge && npm run css:purge",
    "build": "gulp build",
    "go": "gulp go && gulp gogo",
    "git": "git add . && git commit -m",
    "postgit": "git push --all",
    "render": "npm run css:compil && npm run build && npm run css:purge",
    "lunch": "npm run git -- lunch"
  }
}
